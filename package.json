    "babel-preset-stage-2": "^6.24.1",
    "bootstrap-loader": "^2.2.0",
    "bootstrap-sass": "^3.3.7",
    "css-loader": "^0.28.11",
    "eslint": "^3.19.0",
    "eslint-plugin-react": "^7.0.0",
    "expect": "^22.1.0",
    "file-loader": "^0.11.1",
    "import-loader": "^1.0.1",
    "jquery": "^3.3.1",
    "mocha": "^5.0.0",
